{
  "name": "ssh-proxy-server",
  "version": "2.0.0",
  "description": "SSH WebSocket代理服务器",
  "main": "index.js",
  "packageManager": "pnpm@9",
  "scripts": {
    "start": "cross-env DOTENV_CONFIG_PATH=../.env node -r dotenv/config dist/index.js",
    "dev": "cross-env DOTENV_CONFIG_PATH=../.env ts-node-dev -r dotenv/config --respawn --transpile-only index.ts",
    "dev:debug": "cross-env DOTENV_CONFIG_PATH=../.env ts-node-dev -r dotenv/config --inspect --respawn --transpile-only index.ts",
    "build": "tsc -p tsconfig.json && node scripts/copy-assets.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start:dist": "cross-env DOTENV_CONFIG_PATH=../.env node -r dotenv/config dist/index.js",
    "prod": "cross-env NODE_ENV=production DOTENV_CONFIG_PATH=../.env node -r dotenv/config dist/index.js",
    "lint": "pnpm exec eslint . --ext .ts",
    "lint:fix": "pnpm exec eslint . --ext .ts --fix",
    "format": "pnpm -w exec prettier --write \"server/**/*.{ts,js,json,md}\" --ignore-path .prettierignore",
    "format:check": "pnpm -w exec prettier --check \"server/**/*.{ts,js,json,md}\" --ignore-path .prettierignore",
    "deps:check": "pnpm outdated",
    "deps:update": "pnpm update -L",
    "clean": "rimraf logs/*.log",
    "db:backup": "node scripts/backup-db.js",
    "db:restore": "node scripts/restore-db.js"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "@sinclair/typebox": "^0.33.9",
    "archiver": "^7.0.1",
    "axios": "^1.8.0",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.9.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "msgpack5": "^6.0.2",
    "node-cache": "^5.1.2",
    "node-os-utils": "^1.3.7",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.6",
    "ssh2": "^1.16.0",
    "uuid": "^11.1.0",
    "ws": "^8.15.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/ws": "^8.5.10",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "nodemon": "^3.0.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "Apache-2.0"
}

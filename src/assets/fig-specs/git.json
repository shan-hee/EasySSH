{
  "name": "git",
  "description": "分布式版本控制系统",
  "subcommands": [
    { "name": "status", "description": "查看工作区状态" },
    { "name": "add", "description": "添加文件到暂存区" },
    { "name": "commit", "description": "提交更改", "options": [
      { "name": "-m", "description": "提交信息" },
      { "name": "-a", "description": "自动暂存已跟踪文件" },
      { "name": "--amend", "description": "修补上一次提交" }
    ]},
    { "name": "push", "description": "推送到远程", "options": [
      { "name": "-u", "description": "设置上游分支" },
      { "name": "--force-with-lease", "description": "安全强制推送" }
    ]},
    { "name": "pull", "description": "拉取远程更新" },
    { "name": "checkout", "description": "切换/创建分支", "options": [
      { "name": "-b", "description": "创建并切换新分支" }
    ]},
    { "name": "branch", "description": "分支管理" },
    { "name": "log", "description": "查看提交历史", "options": [
      { "name": "--oneline", "description": "单行显示" },
      { "name": "-n", "description": "限制条数" }
    ]},
    { "name": "clone", "description": "克隆仓库" },
    { "name": "fetch", "description": "获取远程更新" },
    { "name": "merge", "description": "合并分支" },
    { "name": "rebase", "description": "变基" },
    { "name": "stash", "description": "暂存当前更改" },
    { "name": "tag", "description": "标签管理" }
  ],
  "options": [
    { "name": "-C", "description": "在指定目录下运行" },
    { "name": "-c", "description": "传递配置项" },
    { "name": "--help", "description": "显示帮助" },
    { "names": ["-v", "--version"], "description": "显示版本号" }
  ]
}

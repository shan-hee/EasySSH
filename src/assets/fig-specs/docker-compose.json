{
  "name": "docker-compose",
  "description": "Docker Compose (旧命令)",
  "subcommands": [
    { "name": "up", "description": "启动服务", "options": [
      { "name": "-d", "description": "后台运行" },
      { "name": "--build", "description": "启动前构建" }
    ]},
    { "name": "down", "description": "停止并移除", "options": [
      { "name": "-v", "description": "移除卷" }
    ]},
    { "name": "logs", "description": "查看服务日志", "options": [
      { "name": "-f", "description": "持续跟随" }
    ]},
    { "name": "start", "description": "启动已创建的服务", "options": [] },
    { "name": "stop", "description": "停止服务", "options": [
      { "name": "-t", "description": "超时时间（秒）" }
    ]},
    { "name": "restart", "description": "重启服务", "options": [
      { "name": "-t", "description": "停止超时时间（秒）" }
    ]},
    { "name": "pause", "description": "暂停服务", "options": [] },
    { "name": "unpause", "description": "恢复服务", "options": [] },
    { "name": "ps", "description": "查看服务容器", "options": [
      { "name": "-a", "description": "显示所有容器" },
      { "name": "--services", "description": "仅列出服务名" }
    ]},
    { "name": "rm", "description": "移除已停止的服务容器", "options": [
      { "name": "-f", "description": "强制移除" },
      { "name": "-s", "description": "移除匿名卷（兼容v2写法）" },
      { "name": "-v", "description": "移除卷" }
    ]},
    { "name": "build", "description": "构建服务镜像", "options": [
      { "name": "--no-cache", "description": "不使用缓存" },
      { "name": "--pull", "description": "总是尝试拉取" },
      { "name": "--parallel", "description": "并行构建" },
      { "name": "--progress", "description": "进度输出 (auto/plain/tty)" },
      { "name": "--build-arg", "description": "传递构建参数 (KEY=VAL)" }
    ]},
    { "name": "create", "description": "创建服务容器(不启动)", "options": [
      { "name": "--force-recreate", "description": "总是重新创建" },
      { "name": "--no-recreate", "description": "已存在则不重建" },
      { "name": "--build", "description": "必要时先构建" },
      { "name": "--remove-orphans", "description": "移除孤立容器" }
    ]},
    { "name": "config", "description": "验证/查看 Compose 配置", "options": [
      { "name": "-q", "description": "仅验证且不输出" },
      { "name": "--services", "description": "仅列出服务" },
      { "name": "--volumes", "description": "仅列出卷" },
      { "name": "--profiles", "description": "仅列出配置的 profiles" }
    ]},
    { "name": "run", "description": "运行一次性容器", "options": [
      { "name": "-d", "description": "后台运行" },
      { "name": "--name", "description": "容器名" },
      { "name": "--rm", "description": "退出后删除容器" },
      { "name": "--service-ports", "description": "映射服务端口" },
      { "name": "-e", "description": "设置环境变量" },
      { "name": "-v", "description": "挂载卷" }
    ]},
    { "name": "exec", "description": "在运行中的容器内执行", "options": [
      { "name": "-it", "description": "交互式终端" },
      { "name": "-u", "description": "切换用户" },
      { "name": "-e", "description": "设置环境变量" }
    ]},
    { "name": "pull", "description": "拉取服务镜像", "options": [
      { "name": "--ignore-pull-failures", "description": "忽略拉取失败" }
    ]},
    { "name": "push", "description": "推送服务镜像", "options": [] },
    { "name": "scale", "description": "按服务伸缩", "options": [] },
    { "name": "top", "description": "显示容器线程信息", "options": [] },
    { "name": "kill", "description": "发送信号结束容器", "options": [
      { "name": "-s", "description": "信号 (e.g. SIGKILL)" }
    ]},
    { "name": "images", "description": "列出服务相关镜像", "options": [] },
    { "name": "version", "description": "显示版本", "options": [] }
  ],
  "options": [
    { "name": "-f", "description": "Compose 文件" },
    { "name": "-p", "description": "工程名" },
    { "name": "--env-file", "description": "环境文件" }
  ]
}

# EasySSH 开发环境配置模板

# 开发环境配置
NODE_ENV=development

# 服务端口配置
VITE_PORT=3000
SERVER_PORT=8000

# API配置
VITE_API_BASE_URL=/api
VITE_API_TARGET=http://localhost:8000

# 开发环境特定配置
VITE_LOG_LEVEL=debug
VITE_DEV_TOOLS=true
VITE_SOURCE_MAP=true

# 缓存配置（开发环境使用较短的缓存时间）
VITE_CACHE_MEMORY_TTL=300000  # 5分钟
VITE_CACHE_SYNC_INTERVAL=60000  # 1分钟

# 开发环境数据库配置
SQLITE_PATH=./server/data/easyssh-dev.sqlite

# 开发环境安全配置（使用简单密钥）
JWT_SECRET=dev-jwt-secret-key
ENCRYPTION_KEY=dev-encryption-key

# 开发环境跨域设置
CORS_ORIGIN=http://localhost:3000

# 开发环境性能配置
SSH_TIMEOUT=5000
SSH_KEEP_ALIVE=15000
MAX_CONNECTIONS_PER_USER=5
